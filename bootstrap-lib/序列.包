!(構造-具型別 '映射 !(構造-序列* !(構造-序列*
  !(構造-序列* '壓入 !(函式 ($甲 $:序列) !(構造-序列/連結 $甲 $:序列)))
  !(構造-序列* '->首 !(函式 ($:序列) !(解構-序列/連結 $:序列 !(函式 ($首 $_) $首))))
  !(構造-序列* '->尾 !(函式 ($:序列) !(解構-序列/連結 $:序列 $_ $尾:序列 $尾:序列)))
  !(構造-序列* '連 !(遞歸 $連 !(函式 ($甲:序列 $乙:序列) !(解構-陰陽 !(序列/空？ $甲:序列)
    $乙:序列
    !(解構-序列/連結 $甲:序列 !(函式 ($甲 $乙) !(構造-序列/連結 $甲 [$連 $乙 $乙:序列])))
  ))))
  !(構造-序列* '內用 !(遞歸 $內用 !(函式 ($:序列 $:函式) !(解構-陰陽 !(序列/空？ $:序列)
    '()
    !(解構-序列/連結 $:序列 !(函式 ($甲 $乙) !(構造-序列/連結 [$:函式 $甲] [$內用 $乙 $:函式])))
  ))))
  !(構造-序列* '瀘過 !(遞歸 $瀘過 !(函式 ($:序列 $:函式) !(解構-陰陽 !(序列/空？ $:序列)
    '()
    !(解構-陰陽 [$:函式 $甲]
      !(解構-序列/連結 $:序列 !(函式 ($甲 $乙) !(構造-序列/連結 $甲 [$瀘過 $乙 $:函式])))
      !(解構-序列/連結 $:序列 !(函式 ($甲 $乙) [$瀘過 $乙 $:函式]))
    )
  ))))
)))
